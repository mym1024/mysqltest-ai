create function func_if3(n int,m int)returns varchar(50)
begin
declare s varchar(50);
if n = m then set s = 'equals';
else
if n>m then set s = 'greater';
else set s = 'less';
end if;
set s = concat('is ',s,' than');
end if;
set s = concat(n,' ',s,' ',m,'.');
return s;
end//
