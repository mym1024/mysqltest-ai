--disable_warnings
drop table if exists t1,t2,t3,t4; 
--enable_warnings


create table t1(i1 int,f1 float,s1 varchar(30),d1 datetime,i2 int,f2 float,s2 varchar(30),d2 datetime,primary key(i1,f1,s1,d1));
prepare stmt_replace from replace into t1 values(?,?,?,?,?,?,?,?);
--let $l=10
while($l)
{
	eval set @i1=$l;
	eval set @f1=$l.5;
	eval set @s1='abcdefg';
	eval set @d1=timestamp'2020-02-02 20:00:00';
	dec $l;
	eval set @i2=$l;
	eval set @f2=$l.5;
	eval set @s2='abcdefgh';
	eval set @d2=timestamp'2020-02-01 20:00:00';
	execute stmt_replace using @i1,@f1,@s1,@d1,@i2,@f2,@s2,@d2;
}
select * from t1;
drop table t1;


