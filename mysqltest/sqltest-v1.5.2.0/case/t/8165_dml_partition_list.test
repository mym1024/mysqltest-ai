###2-1：list分区下，简单的dml操作
--disable_warnings
drop table if exists t1,t2,left_table;
--enable_warnings
create table t1(c0 int primary key,c1 int,c2 int) partition by list(c0) (partition q0 values in(1,3),partition q1 values in (2,4,5));
--real_sleep 2
insert into t1 values(1,10,100);
select c1+10 c3,c2 from t1 where c0=1;
drop table t1;

###2-2:无分区下，简单的dml

--disable_warnings
drop table if exists t1,t2,left_table;
--enable_warnings
create table t1(c0 int primary key,c1 int,c2 int);
insert into t1 values(1,10,100);
select c1+10 c3,c2 from t1 where c0=1;
drop table t1;

