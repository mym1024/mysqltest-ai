###2-1:以datetime类型作为主键，select操作
create table t1(dt datetime primary key,c int);
let $day=12;
while($day <= 14)
{
  let $hour=0;
  while($hour < 24)
  {
    eval insert into t1 values('2012-2-$day $hour:00:00',1);
	inc $hour;
  }
  inc $day;
}
