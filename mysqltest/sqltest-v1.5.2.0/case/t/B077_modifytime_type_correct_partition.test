create table t1(pk int primary key,b modifytime)partition by range(pk) (partition r0 values less than (5),partition r1 values less than (10),partition r2 values less than maxvalue);
insert into t1(pk) values(1);
--real_sleep 1
insert into t1(pk) values(2);
--real_sleep 1

--replace_column 2 searched
select pk,b from t1;
let $value_orig=query_get_value(select * from t1,b,1);

replace into t1(pk) values(1);
--real_sleep 1
replace into t1(pk) values(2);
--real_sleep 1
let $value_now=query_get_value(select * from t1,b,1);
if($value_now == $value_orig)
