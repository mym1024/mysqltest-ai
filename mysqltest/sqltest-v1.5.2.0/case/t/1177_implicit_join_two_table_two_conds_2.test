#####################################Do sql###################################################
alter system set balance_max_concurrent_migrate_num=30 server_type=yaoadminsvr;
alter system set balance_max_migrate_in_per_ds=6 server_type=yaoadminsvr;
alter system set balance_max_migrate_out_per_ds=6 server_type=yaoadminsvr;
alter system set max_migrate_task_count=6 server_type=yaodatasvr;

create database opt;
using database opt;
sleep 5;

###打开查询优化器开关
set yao_enable_query_optimizer=true;

###设置合并参数

###建表插数据
create table if not exists t1(id int primary key, c1 Decimal(5,2), c2 int);
create table if not exists t2(id int primary key, c1 Decimal(5,2), c2 int);

###t1表
###高频值不同值个数：10，高频值平均重复次数：120，低频值不同值个数：100，低频值平均重复次数：10
--let $row_num = 0
--let $HD = 1
--let $LD = 11
while ($HD < 11)
{
  let $number = 0;
  while ($number < 120)
  {
    eval replace into t1 values($row_num, $HD, $HD);
    inc $number;
    inc $row_num;
  }

  if ($number == 120)
  {
    inc $HD;
  }
}

while ($LD < 111)
{
  let $number = 0;
  while ($number < 10)
  {
    eval replace into t1 values($row_num, $LD, $LD);
    inc $number;
    inc $row_num;
  }

  if ($number == 10)
  {
    inc $LD;
  }
}

###t2表
###c1列：高频值不同值个数：10，高频值平均重复次数：50，低频值不同值个数：100，低频值平均重复次数：15
###c2列：高频值不同值个数：10，高频值平均重复次数：100，低频值不同值个数：100，低频值平均重复次数：10
--let $row_num = 0
--let $HD = 1
--let $HD2 = 1
--let $cal = 0
while ($HD < 11)
{
  let $number = 0;
  while ($number < 50)
  {
    eval replace into t2 values($row_num, $HD, $HD2);
    inc $number;
    inc $row_num;
    inc $cal;

    if($cal == 100)
    {
      inc $HD2;
      while ($cal > 0)
      {
        dec $cal;
      }
    }
  }

  if ($number == 50)
  {
    inc $HD;
  }
}

while ($HD < 111)
{
  let $number = 0;
  while ($number < 15)
  {
    eval replace into t2 values($row_num, $HD, $HD2);
    inc $number;
    inc $row_num;
    inc $cal;

    if($row_num <= 999)
    {
       if ($cal == 100)
      {
        inc $HD2;
        while ($cal > 0)
        {
          dec $cal;
        }
      }
    }
    
    if($row_num <= 999)
    {
      if ($cal == 10)
      {
        inc $HD2;
        while ($cal > 0)
        {
          dec $cal;
        }
      }
    }
  }
  
  if ($number == 15)
  {
    inc $HD;
  }
}


###合并
--echo “merge”
exec $LOCAL_DIR/tools/as_admin -r $RS0_IP -p $RS0_PORT major_freeze | tail -1;
--real_sleep 700 
exec $LOCAL_DIR/tools/as_admin -r $RS0_IP -p $RS0_PORT stat -o merge | tail -1;

###生成统计任务
gather statistics t1(c1, c2);
gather statistics t2(c1, c2);

###查看收集任务
select * from __all_udi_monitor_list;

###启动收集命令
--echo “statistics”
exec $LOCAL_DIR/tools/as_admin -r $RS0_IP -p $RS0_PORT gather_statistics | tail -1;
--real_sleep 10 

###查看统计信息
select * from __all_statistic_info;

###检查左右表数据行数
select count(*) from t1;
select count(*) from t2;

###2-1：第一个连接条件为Decimal类型，第二个连接条件为非Decimal类型
###第一个连接条件：左表倾斜率大于0.5，右表倾斜率大于0.5，左右表高频值平均重复次数之积大于10000，
###第二个连接条件：左表倾斜率大于0.5，右表倾斜率大于0.5，左右表高频值平均重复次数之积大于10000
###hash join & merge join => merge join
select count(*) from t1, t2 where t1.c1 = t2.c1 and t1.c2 = t2.c2;
explain select count(*) from t1, t2 where t1.c1 = t2.c1 and t1.c2 = t2.c2;

###第一个连接条件：左表倾斜率大于0.5，右表倾斜率小于0.5，右表不同值个数小于100，
###第二个连接条件：左表倾斜率大于0.5，右表倾斜率大于0.5，左右表高频值平均重复次数之积大于10000
###semi join & merge join => semi join
select count(*) from t1, t2 where t1.c1 = t2.c1 and t1.c2 = t2.c2 and t2.c1 <= 50;
explain select count(*) from t1, t2 where t1.c1 = t2.c1 and t1.c2 = t2.c2 and t2.c1 <= 50;

###第一个连接条件：左表倾斜率大于0.5，右表倾斜率小于0.5，左右表高频值平均重复次数之积小于10000，
###第二个连接条件：左表倾斜率大于0.5，右表倾斜率大于0.5，左表过滤后行数小于200行
###semi join & semi join => semi join
select count(*) from t1, t2 where t1.c1 = t2.c1 and t1.c2 = t2.c2 and t1.c2 = 1;
explain select count(*) from t1, t2 where t1.c1 = t2.c1 and t1.c2 = t2.c2 and t1.c2 = 1;


###删表删库
drop table if exists t1;
drop table if exists t2;
drop database opt;

