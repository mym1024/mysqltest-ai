###2-1:group by的简单使用

create table t1(pk int primary key,grp int,a int,c char(10) not null) partition by range(pk)(partition a1 values  less than(4),partition a2 values less than(8),partition a3 values less than maxvalue);

insert into t1 values(1,1,1,'a');
insert into t1 values(2,2,2,'b');
insert into t1 values(3,2,3,'c');
insert into t1 values(4,3,4,'E');
insert into t1 values(5,3,5,'C');
insert into t1 values(6,3,6,'D');
--real_sleep 1
select a,c,sum(a) from t1 group by a;
select a,c,sum(a) from t1 where a>10 group by a;
select sum(a) from t1 where a>10;
select count(distinct a),count(distinct grp) from t1;
insert into t1 values(7,null,null,'');
select count(distinct a),count(distinct grp) from t1;

create hash partition function h1(x)'x+1';
create table t2(pk int primary key,grp int,a int,c char(10)) partition by (ta2,h1,pk);
select grp,max(a)+max(grp),max(c) from t1 group by grp;
select * from t2;
drop table t1,t2;
create table t1(pk int primary key,id int not null) partition by list(pk)(partition c1 values in(1,2),partition c2 values in(3,4));
create table t2(pk int primary key,id int not null,rating int null) partition by range(pk)(partition d1 values  less than(3),partition d2 values less than(8),partition d3 values less than maxvalue);
--real_sleep 1
insert into t1 values(1,1),(2,2),(3,3);
insert into t2 values(1,1,3),(2,2,NULL),(3,2,NULL),(4,3,2),(5,3,NULL);
--real_sleep 1
select t1.id,avg(rating) from t1 left join t2 on (t1.id=t2.id) group by t1.id;
drop table t1,t2;
create enum partition function e1(x)'[(1)],[(2)],[(4)]';
create table t1(a int primary key,c char(10),b char(128)) partition by (ta1,e1,a);
insert into t1 values(1,'1','1');
insert into t1 values(2,'2','2');
insert into t1 values(4,'4','4');
--real_sleep 1
select count(*) from t1;
select count(*) from t1 where a=1;
select count(*) from t1 where a=100;
select count(*) from t1 where a>=10;
select count(a) from t1 where a=1;
select count(a) from t1 where a=100;
select count(a) from t1 where a>=10;
select count(b) from t1 where b>=2;
select count(b) from t1 where b>=10;
select count(c) from t1 where c=10;
drop table t1;
drop partition function e1;
create hash partition function h2(x)'x-4';
create table t1(d createtime,i int primary key) partition by(ta2,h2,i);
--real_sleep 1
insert into t1(i) values(1);
select count(i),i,count(i)*i from t1 group by i;
select count(i),(i+0),count(i)*(i+0) from t1 group by i;
drop table t1;
drop partition function h2;

