#####################################Do sql###################################################
--disable_warnings
drop table if exists t1;
--enable_warnings

# 建表
create table t1(pk int primary key,c1 int) partition by range(pk)(partition r0 values less than(5),partition r1 values less than(10),partition r2 values less than maxvalue);
--echo "refresh"
exec $LOCAL_DIR/tools/as_admin -r $RS0_IP -p $RS0_PORT refresh_schema | tail -1;
--real_sleep 5
connect(conn1,$OBMYSQL_MS0,admin,admin,test,$OBMYSQL_PORT);

connection conn1;
#
# 2-1:事务开始，提交失败事务，事务回滚
#
begin;
commit;


#
# 2-1:事务回滚后，提交成功事务
--real_sleep 5
--echo insert into 100 rows to ups
--let $count=1
while($count<101)
{
	--let $stmt=INSERT INTO t1 VALUES($count,$count)
	eval $stmt;
	inc $count;
}

--real_sleep 10

--let $i=0
while($i<101)
{
	eval select * from t1;
	inc $i;
}
commit;


