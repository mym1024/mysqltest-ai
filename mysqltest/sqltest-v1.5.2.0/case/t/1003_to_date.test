###2-1 to_date函数的第二个参数为'yyyy-mm-dd'
select to_date('2019-09-18', 'yyyy-mm-dd');
select to_date('2019.09-18', 'yyyy-mm-dd');
select to_date('2019/09-18', 'yyyy-mm-dd');
select to_date('2019,09-18', 'yyyy-mm-dd');
select to_date('2019;09-18', 'yyyy-mm-dd');
select to_date('2019;9-18', 'yyyy-mm-dd');
--error 5663
select to_date('2019-  /  10-  11', 'yyyy-mm-dd');
--error 4190
select to_date('2019-10-11 12:12:23', 'yyyy-mm-dd');
--error 5667
select to_date('2019;87-18', 'yyyy-mm-dd');
select to_date('19;11-18', 'yyyy-mm-dd');
select to_date('1;01-18', 'yyyy-mm-dd'); #月份
--error 5667
select to_date('1;001-18', 'yyyy-mm-dd'); #月份只能是1位或两位非0数字，日期同样
select to_date('20190918', 'yyyy-mm-dd'); #没有分隔符只能是7位或8位
--error 4219
select to_date('20191318', 'yyyy-mm-dd');
--error 4190
select to_date('201909181', 'yyyy-mm-dd');
--error 5667
select to_date('201991', 'yyyy-mm-dd');
--error 5663
select to_date('qqq', 'yyyy-mm-dd');
--error 5663
select to_date('sasdghyu', 'yyyy-mm-dd');

--error 4219
select to_date('19-123', 'yyyy-mm-dd'); #只有一个分隔符，且分隔符之前有小于4位数字，分隔符之后有超过2位数字但小于4位
--error 5639
select to_date('0-123', 'yyyy-mm-dd'); #年份不能为0
--error 4219
select to_date('11-123', 'yyyy-mm-dd');
--error 5638
select to_date('9999-12', 'yyyy-mm-dd'); #分隔符之后有小于等于2位数字
--error 5667
select to_date('9999-1234', 'yyyy-mm-dd'); #分隔符之后的位数正确但不在合理的日期范围之内
--error 4219
select to_date('9999-1409', 'yyyy-mm-dd'); #分隔符之后的位数正确但不在合理的日期范围之内
--error 4190
select to_date('9999-140911', 'yyyy-mm-dd');

