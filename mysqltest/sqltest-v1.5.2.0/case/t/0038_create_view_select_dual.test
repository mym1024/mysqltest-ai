create view v1 as select t2.c1+t1.c2 from (select 1.236*2.78 as c1,c2 from (select 15.23333 as c2)) t2,(select c2,15*38.258 from t1) t1 where 1<10 or 20.134 <159 or 2.356798789 *23326 >=3;
