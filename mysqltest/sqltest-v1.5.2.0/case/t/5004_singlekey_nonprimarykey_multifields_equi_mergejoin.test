###1.1.测试功能点:两张表merge_join，表的主键为单主键；仅有一个连接条件，连接字段为两表的非主键；等值连接
#######################################################
--disable_warnings
drop table if exists t1,t2,t3,t4;
--enable_warnings

create table t1(id_p int primary key, lastname varchar(20), firstname varchar(20), address varchar(20), city varchar(20), id_n int);
insert into t1 values(1,'a','a1','a2','a3',3);
insert into t1 values(2,'b','b1','b2','b3',3);
insert into t1 values(3,'b','b1','b2','b3',3);

create table t2(id int primary key,name varchar(255) default NULL);
insert into t2 values(1,'a1'),(2,'a2'),(3,'a3'),(4,'a4'),(5,'a5');

create table t3(id_o int primary key, orderno int, id_p int, id_n int);
insert into t3 values(1,1111,3,2);
insert into t3 values(2,2222,2,1);
insert into t3 values(3,3333,1,3);
insert into t3 values(4,4444,1,4);
insert into t3 values(5,5555,65,3);

create table t4(id int, category int, county int, state int, phones int, nophones int, id_n int, primary key(id));
insert into t4 values(3,2,11,12,2222,7800,1);
insert into t4 values(4,2,25,12,6500,11200,3);
insert into t4 values(5,1,37,6,10000,12000,2);

###2-1:���ű�merge join����������Ϊ��������һ�����������������ֶ�Ϊ���ű��ķ�����
#select /*+ join(hash_join_single) */* from t1 full join t2 on t1.city=t2.name;
select /*+ join(merge_join) */* from t1 inner join t2 on t1.city=t2.name;
select /*+ join(merge_join) */* from t1 left join t2 on t1.city=t2.name;
select /*+ join(merge_join) */* from t1 right join t2 on t1.city=t2.name;
select /*+ join(merge_join) */* from t1 full join t2 on t1.city=t2.name;

#select /*+ join(hash_join_single) */* from t3 full join t4 on t4.id_n=t3.id_n;
select /*+ join(merge_join) */* from t3 inner join t4 on t4.id_n=t3.id_n;
select /*+ join(merge_join) */* from t3 left join t4 on t4.id_n=t3.id_n;
select /*+ join(merge_join) */* from t3 right join t4 on t4.id_n=t3.id_n;
select /*+ join(merge_join) */* from t3 full join t4 on t4.id_n=t3.id_n;

###2-2:���ű�merge join����������Ϊ��������������������������ֶ�Ϊ���ű��ķ�����
#select /*+ join(hash_join_single) */* from t3 full join t4 on t4.id_n=t3.id_n and t3.id_p=t4.category;
select /*+ join(merge_join) */* from t3 inner join t4 on t4.id_n=t3.id_n and t3.id_p=t4.category;
select /*+ join(merge_join) */* from t3 left join t4 on t4.id_n=t3.id_n and t3.id_p=t4.category;
select /*+ join(merge_join) */* from t3 right join t4 on t4.id_n=t3.id_n and t3.id_p=t4.category;
select /*+ join(merge_join) */* from t3 full join t4 on t4.id_n=t3.id_n and t3.id_p=t4.category;

#select /*+ join(hash_join_single) */* from t3 full join t4 on t4.id_n=t3.id_n and t3.p=t4.category and t3.orderno=t4.phones;
select /*+ join(merge_join) */* from t3 inner join t4 on t4.id_n=t3.id_n and t3.id_p=t4.category and t3.orderno=t4.phones;
select /*+ join(merge_join) */* from t3 left join t4 on t4.id_n=t3.id_n and t3.id_p=t4.category and t3.orderno=t4.phones;
