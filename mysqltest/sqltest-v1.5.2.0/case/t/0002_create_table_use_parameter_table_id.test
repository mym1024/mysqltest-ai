alter system set min_drop_cache_wait_time='10s' server_type=yaodatasvr;
alter system set each_tablet_sync_meta=False server_type=yaodatasvr;

###2-1：(创建表时不能指定已经存在的表的table_id)
##do sql 
#table_id 1
create table t1(i1 int primary key, i2 int, i3 int, v1 varchar(20));
create table t2(i1 int primary key, i2 int, i3 int, v1 varchar(20));
select table_name,table_id from __first_tablet_entry where table_name like 't_';
let $n=query_get_value(select value from __all_sys_stat where name='yao_max_used_table_id',value,1);
--error 5020
eval create table t3(i1 int primary key, i2 int, i3 int, v1 varchar(20)) table_id=$n;
select table_name,table_id from __first_tablet_entry where table_name like 't_';
drop table t1,t2;

###2-2：(建表时指定table_id不能大于‘yao_max_used_table_id’的值)
##do sql 
#table_id 2
create table t1(i1 int primary key, i2 int, i3 int, v1 varchar(20));
create table t2(i1 int primary key, i2 int, i3 int, v1 varchar(20));
select table_name,table_id from __first_tablet_entry where table_name like 't_';
let $n=query_get_value(select value from __all_sys_stat where name='yao_max_used_table_id',value,1);
inc $n;
--error 65535
eval create table t3(i1 int primary key, i2 int, i3 int, v1 varchar(20)) table_id=$n;
drop table t1,t2;

###2-3：(创建表时指定被删除的表的table_id,必须要在表被删除后进行合并)
##do sql 
#table_id 3
create table t1(i1 int primary key, i2 int, i3 int, v1 varchar(20));
create table t2(i1 int primary key, i2 int, i3 int, v1 varchar(20));
select table_name,table_id from __first_tablet_entry where table_name like 't_';
create table t3(i1 int primary key, i2 int, i3 int, v1 varchar(20));
let $id=query_get_value(select table_id from __first_tablet_entry where table_name='t2',table_id,1);
drop table t2;
--error 54
eval create table t2(i1 int primary key, i2 int, i3 int, v1 varchar(20)) table_id=$id;
exec  $LOCAL_DIR/tools/as_admin -r $RS0_IP -p $RS0_PORT major_freeze | tail -1;
--real_sleep 400
eval create table t2(i1 int primary key, i2 int, i3 int, v1 varchar(20)) table_id=$id;
insert into t2 values(1,2,3,'a');
select * from t2;
select table_name,table_id from __first_tablet_entry where table_name like 't_';
drop table t1,t2,t3;

