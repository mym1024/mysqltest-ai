select to_date('2005-01-01','yyyy-MM-dd') test1 from dual;
select to_date('19950101','yyyy-mm-dd') test2 from dual;
SELECT TO_DATE('20240408', 'YYYYMMDD') test3 FROM DUAL;
SELECT TO_DATE('2022-12-15 13:45:30', 'YYYY-MM-DD HH24:MI:SS') test4 FROM DUAL;
select to_date('2024-01-01 08:00:00', 'YYYY-MM-DD HH24:MI:SS') - to_date('2024-01-02 08:00:00', 'YYYY-MM-DD HH24:MI:SS') test5 FROM dual;
select to_date('2024-02-04 08:00:00', 'YYYY-MM-DD HH24:MI:SS') - to_date('2024-02-01 08:00:00', 'YYYY-MM-DD HH24:MI:SS') test6 FROM dual;
select ROUND((to_date('2024-01-01 08:00:00', 'YYYY-MM-DD HH24:MI:SS') - to_date('2024-01-02 08:00:00', 'YYYY-MM-DD HH24:MI:SS'))*24,2) test7 FROM dual;
select to_char(to_date(concat('2024', '-01-01 00:00:00'),'yyyy-MM-dd HH24:mi:ss'),'yyyy-mm-dd') test8 from dual;
SELECT TO_DATE('199912','YYYYMM'),TO_DATE('2000.05.20','YYYY.MM.DD'),(DATE '2008-12-31') XXDATE,TO_DATE('2008-12-31 12:31:30','YYYY-MM-DD HH24:MI:SS'),(TIMESTAMP '2008-12-31 12:31:30') XXTIMESTAMP FROM DUAL;
SELECT TO_NUMBER('199912'),TO_NUMBER('450.05') FROM DUAL;
SELECT TO_NUMBER(NULL) AS null_value FROM dual;
SELECT TO_NUMBER('100.50') + TO_NUMBER('200.75') AS total FROM dual;
SELECT TO_NUMBER('4') * TO_NUMBER('2.5') AS total FROM dual;
SELECT TO_NUMBER('4') / TO_NUMBER('2.5') AS total FROM dual;
SELECT TO_NUMBER('100.50') - TO_NUMBER('200.75') AS total FROM dual;
