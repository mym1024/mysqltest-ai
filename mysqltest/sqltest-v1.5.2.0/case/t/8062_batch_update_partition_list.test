--disable_warnings
drop table if exists test1,test2;
--enable_warnings

alter system set min_drop_cache_wait_time='10s' server_type=yaodatasvr;


# 主键是int类型
create table test1(c1 int, c2 varchar(20), c3 decimal(10,5), c4 int, primary key(c1)) partition by list(c1) (partition q0 values in(1,2,3,4,5));
show table rules;
insert into test1 values(1,'a',1,1),(2,'b',2,2),(3,'c',3,3),(4,'d',4,4);
show partition groups;
create index idx1 on test1(c1);
create index idx2 on test1(c2);
create index idx3 on test1(c3);
create index idx4 on test1(c4);

# 主键是int,varchar和decimal类型
create table test2(c1 int, c2 decimal(10,5), c3 varchar(20), c4 int, c5 int, c6 int, c7 decimal(10,5), c8 varchar(20), primary key(c1,c2,c3)) partition by list(c1) (partition q0 values in(1,2,3,4,5));
insert into test2 values(1,1.11,'a',1,1,1,1,1),(2,2.22,'b',2,2,2,2,2),(3,3.33,'c',3,3,3,3,3),(4,4.44,'b',4,4,4,4,4);
create index idx2 on test2(c5) storing(c6,c4);
create index idx3 on test2(c2,c3) storing(c4,c5);

--echo merge
exec $LOCAL_DIR/tools/as_admin -r $RS0_IP -p $RS0_PORT major_freeze | tail -1;
