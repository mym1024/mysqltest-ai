###2-5：不指定分区表中使用prepare执行insert操作
create table t1(i1 int,f1 float,s1 varchar(30),d1 datetime,i2 int,f2 float,s2 varchar(30),d2 datetime,primary key(i1,f1,s1,d1));
--real_sleep 5
prepare stmt_replace from replace into t1 values(12341234,1234.1234,'1234.1234','2012-03-04 12:03:04',?,?,?,?);
--let $l=10
while($l)
{
	dec $l;
	eval set @i2=$l;
	eval set @f2=$l.5;
	eval set @s2='$l.abcdefg';
	eval set @d2=timestamp'2020-02-01 20:00:00';
	execute stmt_replace using @i2,@f2,@s2,@d2;
}
select * from t1;
drop table t1;


create table t1(i1 int,f1 float,s1 varchar(30),d1 datetime,i2 int,primary key(i1,f1,s1,d1));
