sleep 5;
--disable_warnings
drop table if exists t1,t2,t3,t4;
--error 5077
drop partition function h1,e1;
--enable_warnings

create hash partition function h1(x)'x+7';
create table t1 (id_p int primary key, lastname varchar(20), firstname varchar(20), address varchar(20), city varchar(20), id_n int) partition by(ta1,h1,id_p);
--real_sleep 5
insert into t1 values(1,'a','a1','a2', 'a3',3);
insert into t1 values(2,'b','b1','b2', 'b3',3);
insert into t1 values(3,'c','c1','c2', 'c3',2);


create table t2 (id_o int primary key, orderno int, id_p int, id_n int) partition by range(id_o)(partition a1 values less than(3),partition a2 values less than(6),partition a3 values less than maxvalue);
--real_sleep 5
insert into t2 values (1,1111,3,2);
insert into t2 values (2,2222,3,1);
insert into t2 values (3,3333,1,3);
insert into t2 values (4,4444,1,4);
insert into t2 values (5,5555,65,3);

create enum partition function e1(x) '[(3)],[(4)],[(5)]';
create table t3 (
  id int,
  category int,
  county int,
  state int,
  phones int,
  nophones int,
  id_n int,
  primary key (id)
) partition by(ta3,e1,id);
--real_sleep 5
insert into t3 values (3,2,11,12,5400,7800,1);
insert into t3 values (4,2,25,12,6500,11200,3);
insert into t3 values (5,1,37,6,10000,12000,2);

create table t4 (
  id int,
  name varchar(255) default NULL,
  primary key (id)
) partition by list(id)(partition b1 values in(1,3,5),partition b2 values in(2,4));
--real_sleep 5
insert into t4 values (1,'s1'),(2,'s2'),(3,'s3'),(4,'s4'),(5,'s5');

select /*+ join(si) */ * from t1 left join t2 on t1.id_p=t2.id_p;
select /*+ join(si) */ * from t1 inner join t2 on t1.id_p=t2.id_p;
--error 5205
select /*+ join(si) */ * from t1 right join t2 on t1.id_p=t2.id_p;
select /*+ join(si) */ * from t2 left join t3 on t2.id_o=t3.id_n;
select /*+ join(si) */ * from t2 full join t3 on t2.id_o=t3.id_n;
select /*+ join(si) */ * from t2 inner join t3 on t2.id_p=t3.id;
--error 5205
select /*+ join(si) */ * from t2 right join t3 on t2.id_p=t3.id;
select /*+ join(si) */ * from t1,t2 where t1.id_p=t2.id_p and t1.id_p=t2.id_n;

select /*+ join(merge_join) */ * from t1 left join t2 on t1.id_p=t2.id_p;
select /*+ join(merge_join) */ * from t1 inner join t2 on t1.id_p=t2.id_p;
select /*+ join(merge_join) */ * from t1 right join t2 on t1.id_p=t2.id_p;
select /*+ join(merge_join) */ * from t2 left join t3 on t2.id_o=t3.id_n;
select /*+ join(merge_join) */ * from t2 full join t3 on t2.id_o=t3.id_n;
select /*+ join(merge_join) */ * from t2 inner join t3 on t2.id_p=t3.id;
select /*+ join(merge_join) */ * from t2 right join t3 on t2.id_p=t3.id;
select /*+ join(merge_join) */ * from t1,t2 where t1.id_p=t2.id_p and t1.id_p=t2.id_n;

