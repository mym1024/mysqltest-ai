###2-1：single field

create table t1(a varchar(50) primary key,b int default 0);
select * from t1;
select * from t1 where a > -1;
select * from t1 where a = -1;
select * from t1 where a < -1;
select * from t1 where a > 0;
select * from t1 where a = 0;
select * from t1 where a < 0;
select * from t1 where a > 1;
select * from t1 where a = 1;
select * from t1 where a < 1;

let $i=10;
disable_query_log;
while($i > -10)
{
eval insert into t1(a) values('$i');
dec $i;
}
enable_query_log;
select * from t1;
select * from t1 where a > -1;
select * from t1 where a = -1;
select * from t1 where a < -1;
select * from t1 where a > 0;
select * from t1 where a = 0;
select * from t1 where a < 0;
select * from t1 where a > 1;
select * from t1 where a = 1;
select * from t1 where a < 1;

drop table t1;

