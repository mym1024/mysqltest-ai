create table t1 (c1 int primary key ,c2 date);

###2-1:to_date函数优化
insert into t1 values(1,'2013-12-22');

select to_date(c2,'yyyy-mm-dd') from t1 limit 1;

--error 2
select to_date('yyyymmdd',c2) from t1 limit 1;

###2-2:to_timestamp函数优化

--error 5102
select to_timestamp(c2,'yyyy-mm-dd hh:mm:ss') from t1 limit 1;

--error 2
select to_timestamp('yyyy-mm-dd hh:mm:ss',c2) from t1 limit 1;

###2-3:date_add函数优化
select date_add(c2, interval 1 days) from t1;

--error 5001
select date_add(interval 1 days,c2) from t1;

select date_add(date '2013-12-22', interval 1 days);
--error 5001
select date_add(interval 1 days,date '2013-12-22');

drop table t1;



