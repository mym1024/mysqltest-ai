--disable_warnings
drop table if exists list1,list2;
--enable_warnings

#键值为int
# rowkey use int and varchar
create table list1(c1 int,c2 varchar(20),c3 decimal(10,5),c4 int,primary key(c1)) partition by list(c1) (partition q0 values in(1,2,3,4));
show table rules;
insert into list1 values(1,'a',1,1),(2,'b',2,2),(3,'c',3,3),(4,'d',4,4);
show partition groups;
create index idx1 on list1(c1);
create index idx2 on list1(c2);
create index idx3 on list1(c3);

#键值为int,varchar and decimal
# rowkey use int,varchar and decimal
create table list2(c1 int,c2 decimal(10,5),c3 varchar(20),c4 int,c5 int,c6 int,c7 decimal(10,5),c8 varchar(20),primary key(c1,c2,c3)) partition by list(c1) (partition q0 values in(1,2,3,4));
insert into list2 values(1,1.11,'a',1,1,1,1,1),(2,2.22,'b',2,2,2,2,2),(3,3.33,'c',3,3,3,3,3),(4,4.44,'b',4,4,4,4,4);
create index idx2 on list2(c5) storing(c6,c4);
create index idx3 on list2(c2,c3) storing(c4,c5);

alter system set min_drop_cache_wait_time='10s' server_type=yaodatasvr;

--echo merge
