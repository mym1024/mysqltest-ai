let $1=100;
let $2=200;
disable_query_log;
while ($1)
{
 eval insert into a1(rowkey_suffix,price) values($2,$1);
 dec $2;
 eval insert into a1(rowkey_suffix,price) values($2,$1);
 dec $1;
 dec $2;
}
enable_query_log;
select count(distinct price) from a1;



