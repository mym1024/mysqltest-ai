--disable_warnings
drop table if exists t1,t2,t3,t4; 
--enable_warnings


###2-1：对空表进行desc操作
create table t1(i1 int,f1 float,s1 varchar(255),d1 datetime,i2 int,f2 float,s2 varchar(30),d2 datetime,primary key(i1,f1,s1,d1));
desc t1;


###2-2：对insert后的表进行desc操作
insert into t1(i1,f1,s1,d1,i2,f2,s2,d2) values(1,1.2345,'row_columns3','2020-02-01 20:00:00',2,2.34542,'row_columns7','2014-04-01 20:00:00'),(2,1.2345231,'row_columns3','2014-04-01 20:00:00',3,2.34542,'row_columns7','2014-04-01 20:00:00');
select * from t1;
desc t1;


###2-3：对update后的表进行desc操作
update t1 set i2=10,f2=10.1,s2='be_updated',d2='2011-11-11 10:00:00' where i1=1 and f1=1.2345 and s1='row_columns3' and d1='2020-02-01 20:00:00';
desc t1;


###2-4：对delete后的表进行desc操作
delete from t1 where i1=1 and f1=1.2345 and s1='row_columns3' and d1='2020-02-01 20:00:00';
desc t1;

delete from t1 where i1=2 and f1=1.2345231 and s1='row_columns3' and d1='2014-04-01 20:00:00';
select * from t1;
desc t1;
drop table t1;

create table t2(s varchar(65535) primary key,a int);
desc t2;
drop table t2;


