###2-1:在range分区规则下，insert不同类型的数据
--disable_warnings
drop table if exists trange1;
--enable_warnings

create table trange1(c1 int primary key,c2 datetime,c3 varchar(30)) partition by range(c1) (partition r0 values less than(200),partition r1 values less than maxvalue);
show table rules;
insert into trange1 select cast('123' as int),cast('2015-04-06 12:56:45' as datetime),cast(123 as varchar(30));
--real_sleep 5
select * from trange1;
show partition groups;


###2-2:在list分区规则下，insert不同类型的数据
--disable_warnings
drop table if exists list1;
--enable_warnings

create table list1(c1 int primary key,c2 datetime,c3 varchar(30)) partition by list(c1) (partition q0 values in(123));
show table rules;
--real_sleep 5
insert into list1 select cast('123' as int),cast('2015-04-06 12:56:45' as datetime),cast(123 as varchar(30));
select * from list1;
show partition groups;

###2-3:在enum分区规则下，insert不同类型的数据
