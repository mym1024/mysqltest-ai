create enum partition function e(x) '[(123)]';
--enable_warnings

create table t11(c1 int primary key,c2 datetime,c3 varchar(30)) partition by (em,e,c1);
--real_sleep 5
insert into t11 select cast('123' as int),cast('2015-04-06 12:56:45' as datetime),cast(123 as varchar(30));
select * from t11;
show partition groups;


###2-4:在hash分区规则下，insert不同类型的数据
--disable_warnings
drop table if exists t1,t2;
--error 5077
drop partition function h;
create hash partition function h(x) 'x+1';
--enable_warnings

create table t1(c1 int primary key,c2 datetime,c3 varchar(30)) partition by (hs,h,c1);
--real_sleep 5
insert into t1 select cast('123' as int),cast('2015-04-06 12:56:45' as datetime),cast(123 as varchar(30));
select * from t11;
show partition groups;

insert into t1(c1,c2) select 2,current_timestamp();
select c1,c3 from t1;
--real_sleep 5
insert into t1(c1,c3) select length('hash'),substr('123456789',2,4);
select c1,c3 from t1;

###2-5:在不指定分区规则的情况下，insert不同类型的数据
--disable_warnings
drop table if exists t1,t2;
--enable_warnings
create table t1(c1 int,c2 int,c3 datetime,c4 varchar(30),primary key(c1,c2));
create table t2(c1 int primary key,c2 int);
--real_sleep 5
insert into t2 values(1,1),(2,2),(3,3),(4,4);
select * from t2;
insert into t1(c1,c2) select avg(c1),count(*) from t2;
insert into t1(c1,c2) select sum(c2),max(c1) from t2;
select * from t1;
insert into t1(c1,c2) select max(c1),min(c1) from t2;
select * from t1;

