#####################################Do sql####################################################
drop table if exists c,d;
create table c (c1 int primary key, c2 varchar(8)) partition by list(c1)(partition a1 values in(1,2),partition a2 values in(3,4));
create table d (d1 int primary key, d2 varchar(8)) partition by range(d1)(partition b1 values less than(2),partition b2 values less than(4),partition b3 values less than maxvalue);
set autocommit= 0;
--real_sleep 5
insert into c values(1,1);
insert into d values(1,1);
select * from c,d where c.c1 = d.d1;
select * from c
left join d on c.c1 = d.d1;
select * from c
right join d on c.c1 = d.d1;
select * from c
inner join d on c.c1 = d.d1;
select/*+join(si)*/ * from c
left join d on c.c1 = d.d1;
select/*+join(bloomfilter_join)*/ * from c
left join d on c.c1 = d.d1;
commit;
drop table c,d;
