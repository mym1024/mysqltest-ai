###2-1 建有range分区的表后向表中插入大小超过1M的行
--let $const=12
--let $create_sql=create table t1(pk int primary key
--let $create_sql=select pk
--let $field=$const
while($field)
{
   --let $create_sql=$create_sql , f$field varchar(70000)
   --let $select_sql=$select_sql , substr(f$field,0,5)
   dec $field;
}
--let $create_sql=$create_sql ) partition by range(pk)(partition z1 values less than(5),partition z2 values less than(10),partition z3 values less than maxvalue);
--let $select_sql=$select_sql from t1
eval $create_sql;
exec $LOCAL_DIR/tools/as_admin -r $RS0_IP -p $RS0_PORT refresh_schema | tail -1;
--real_sleep 3

--let $insert_sql=insert into t1 values(1

