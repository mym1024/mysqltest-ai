--let $
while($kb)
{
   --let $value=$value$byte
   dec $kb;
}

--let $field=$const
while($field)
{
   --let $insert_sql=$insert_sql , '$value'
   dec $field;
}
--let $insert_sql=$insert_sql )
--disable_query_log
eval $insert_sql;
