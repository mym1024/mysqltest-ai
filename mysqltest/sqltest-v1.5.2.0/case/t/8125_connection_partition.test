###2-1:hash分区规则下
--error 5077
drop partition function h1;
create hash partition function h2(x) 'x+1';
create table t1(a int primary key,b int) partition by(h2,h2,a);
let $a=1;
--real_sleep 5
--disable_query_log
while($a<1000)
{
eval insert into t1(a) values($a);
inc $a;
}
--enable_query_log
disconnect conn1;

connection conn2;
select * from t1;
drop table t1;
disconnect conn2;
connection conn3;
