--disable_warnings
drop table if exists t1,t2;
--enable_warnings
create table t1(c1 int,c2 int,c3 datetime,c4 varchar(30),primary key(c1,c2));
create table t2(c1 int primary key,c2 int);
insert into t2 values(1,1),(2,2),(3,3),(4,4);
select * from t2;
insert into t1 (c1,c2) select avg(c1),count(*) from t2;
insert into t1 (c1,c2) select sum(c2),max(c1) from t2;
select * from t1;

insert into t1 (c1,c2) select max(c1),min(c1) from t2;
select * from t1;

