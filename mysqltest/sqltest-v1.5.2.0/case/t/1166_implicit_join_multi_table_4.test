###设置合并参数

###建表插数据
create table if not exists t1(id int primary key, c1 Decimal(5,2), c2 int);
create table if not exists t2(id int primary key, c1 Decimal(5,2), c2 int);
create table if not exists t3(id int primary key, c1 Decimal(5,2), c2 int);

###t1表
###t1.c1，t1.c2：高频值不同值个数：10，高频值平均重复次数：400，低频值不同值个数：300，低频值平均重复次数：30
--let $row_num = 0
--let $HD = 1
--let $LD = 11
while ($HD < 11)
{
  let $number = 0;
  while ($number < 400)
  {
    eval replace into t1 values($row_num, $HD, $HD);
    inc $number;
    inc $row_num;
  }

  if ($number == 400)
  {
    inc $HD;
  }
}

while ($LD < 311)
{
  let $number = 0;
  while ($number < 30)
  {
    eval replace into t1 values($row_num, $LD, $LD);
    --real_sleep 0.1
    inc $number;
    inc $row_num;
  }

  if ($number == 30)
  {
    inc $LD;
  }
}

